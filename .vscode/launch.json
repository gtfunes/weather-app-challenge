    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Android",
            "cwd": "${workspaceFolder}",
            "type": "reactnative",
            "request": "launch",
            "platform": "android",
            "enableDebug": false,
        },
        {
            "name": "Run Android (Debug)",
            "cwd": "${workspaceFolder}",
            "type": "reactnative",
            "request": "launch",
            "platform": "android",
            "enableDebug": true,
        },
        {
            "name": "Run iOS (Debug)",
            "cwd": "${workspaceFolder}",
            "type": "reactnative",
            "request": "launch",
            "platform": "ios",
            "enableDebug": true,
        },
        {
            "name": "Run iOS",
            "cwd": "${workspaceFolder}",
            "type": "reactnative",
            "request": "launch",
            "platform": "ios",
            "enableDebug": false,
        }
    ]
}